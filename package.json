{
  "name": "parque-arauco-cms",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "lint": "tslint --project .",
    "test:report": "node scripts/test.js --env=jsdom --coverage",
    "lint:report": "tslint -t json -o tsreport.json --project .",
    "precommit": "pretty-quick --staged",
    "prepush": "npm run lint:report && npm run test:report"
  },
  "dependencies": {
    "@types/dom-to-image": "^2.6.0",
    "@types/enquire.js": "^2.1.1",
