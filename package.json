    "@types/prop-types": "^15.5.5",
    "@types/react-intl": "^2.3.11",
    "@types/react-leaflet": "^1.1.6",
    "@types/react-loadable": "^5.4.2",
    "@types/react-redux": "^6.0.9",
    "@types/react-router": "^4.0.31",
    "@types/react-router-redux": "^5.0.16",
    "antd": "^3.10.3",
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-jest": "^22.1.0",
